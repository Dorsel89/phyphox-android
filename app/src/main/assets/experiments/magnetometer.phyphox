<phyphox version="1.0">
    <title>Magnetometer</title>
    <category>Raw Sensors</category>
    <description>Get raw data from the magnetometer.</description>
    <translations>
        <translation locale="de">
            <title>Magnetfeld</title>
            <category>Sensoren</category>
            <description>Rohdaten des Magnetometers auslesen.</description>
            <string original="Raw Data">Rohdaten</string>
        </translation>
    </translations>
    <data-containers>
        <container size="500">mag_time</container>
        <container size="500">magX</container>
        <container size="500">magY</container>
        <container size="500">magZ</container>
    </data-containers>
    <input>
        <sensor type="magnetic_field" rate="25">
            <output component="x">magX</output>
            <output component="y">magY</output>
            <output component="z">magZ</output>
            <output component="t">mag_time</output>
        </sensor>
    </input>
    <views>
        <view label="Raw Data">
            <graph label="Magnetometer X" labelX="t (s)" labelY="B (µT)" partialUpdate="true">
                <input axis="x">mag_time</input>
                <input axis="y">magX</input>
            </graph>
            <graph label="Magnetometer Y" labelX="t (s)" labelY="B (µT)" partialUpdate="true">
                <input axis="x">mag_time</input>
                <input axis="y">magY</input>
            </graph>
            <graph label="Magnetometer Z" labelX="t (s)" labelY="B (µT)" partialUpdate="true">
                <input axis="x">mag_time</input>
                <input axis="y">magZ</input>
            </graph>
        </view>
    </views>
    <export>
        <set name="Raw Data">
            <data name="Time (s)">mag_time</data>
            <data name="Magnetic Field x (µT)">magX</data>
            <data name="Magnetic Field y (µT)">magY</data>
            <data name="Magnetic Field z (µT)">magZ</data>
        </set>
    </export>
</phyphox>
